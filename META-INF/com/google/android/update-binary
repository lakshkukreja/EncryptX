unpack_package_files() {

# Delete TMP Package Directory
rm -rf /tmp/EncryptX
# Create TMP Package Directory
mkdir -p /tmp/EncryptX

# Extract Source Files
package_extract_sourcefile "EncryptX/tools/busybox" "/tmp/EncryptX/busybox"
package_extract_sourcefile "EncryptX/tools/fec" "/tmp/EncryptX/fec"
package_extract_sourcefile "EncryptX/tools/httools_static" "/tmp/EncryptX/httools_static"
package_extract_sourcefile "EncryptX/tools/lptools_static" "/tmp/EncryptX/lptools_static"
package_extract_sourcefile "EncryptX/tools/magiskboot" "/tmp/EncryptX/magiskboot"
package_extract_sourcefile "EncryptX/tools/magiskpolicy" "/tmp/EncryptX/magiskpolicy"
package_extract_sourcefile "EncryptX/tools/snapshotupdater_static" "/tmp/EncryptX/snapshotupdater_static"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/$FILE_INFO"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/user_serial_verified"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/user_serial_unverified"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/user_soc_serial_verified"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/user_soc_serial_unverified"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/device_model_verified"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/device_model_unverified"
if [ $EncryptXModule_ACTIVATE_CRS = 1 ]; then
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/recovery_version_verified"
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/recovery_version_unverified"
else
package_extract_sourcefile "EncryptX/tools/placeholder" "/tmp/EncryptX/recovery_checker_locked"
fi

chmod 755 /tmp/EncryptX/busybox
chmod 755 /tmp/EncryptX/fec
chmod 755 /tmp/EncryptX/httools_static
chmod 755 /tmp/EncryptX/lptools_static
chmod 755 /tmp/EncryptX/magiskboot
chmod 755 /tmp/EncryptX/magiskpolicy
chmod 755 /tmp/EncryptX/snapshotupdater_static
chmod 755 /tmp/EncryptX/user_serial_verified
chmod 755 /tmp/EncryptX/user_serial_unverified
chmod 755 /tmp/EncryptX/user_soc_serial_verified
chmod 755 /tmp/EncryptX/user_soc_serial_unverified
chmod 755 /tmp/EncryptX/device_model_verified
chmod 755 /tmp/EncryptX/device_model_unverified
chmod 755 /tmp/EncryptX/recovery_version_verified
chmod 755 /tmp/EncryptX/recovery_version_verified

setup_keycheck_extraction() {
# Perform Partiality Extraction Case For Keycheck Method!
package_extract_sourcefile "EncryptX/tools/keycheck" "/tmp/EncryptX/keycheck"
copyfile "/tmp/EncryptX/keycheck" "/tmp/bin/android/keycheck"
chmod 755 /tmp/bin/android/keycheck

# Revise Directory 'bin' for keycheck config
bin=/tmp/bin/android;
}

}

