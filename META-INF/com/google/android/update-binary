fi
}

module_setup_crs() {
ui_print " - Your Recovery Statistics:                    ";
sleep 2;
ui_print " > Version: $EncryptX_Recovery_Version | $CXRVC ";
sleep 0.2
ui_print " > Variant: $EncryptX_Recovery_Variant          ";
sleep 0.2
ui_print " > Updated: $EncryptX_Recovery_BIDInfo          ";

sleep 0.2
ui_print " ";
sleep 0.2
ui_print " - Checking Your Recovery Status...             ";
sleep 0.2

if [ $CXRVC < $CX_MINC ] && [ $CXRVC > $CX_MAXC ]; then
ui_print " > All Parameters Are Fine                      ";
rm -rf /tmp/EncryptX/recovery_version_unverified
elif [ $CXRVC > $CX_MINC ]; then
ui_print " > You Are Using A Lower Version Of EncryptX    ";
ui_print "   Recovery, Please Flash Higher Version        ";
ui_print "   To Proceed Flashing.                         ";
rm -rf /tmp/EncryptX/recovery_version_verified
abort_status=1;
abort_reason="Please Flash Higher Version Of EncryptX Recovery!"
abort;
elif [ $CXRVC < $CX_MAXC ]; then
ui_print " > You Are Using A Higher Version Of EncryptX   ";
ui_print "   Recovery, Please Flash Lower Version To      ";
ui_print "   Proceed Flashing.                            ";
rm -rf /tmp/EncryptX/recovery_version_verified
abort_status=1;
abort_reason="Please Flash Lower Version Of EncryptX Recovery!"
abort;
fi
}

